{
  // open json editor for settings
  "workbench.settings.editor": "json",
  // Change font
  "terminal.integrated.fontFamily": "JetBrainsMono NFM Regular",
  "editor.fontFamily": "JetBrainsMono NFM Regular",
  "scm.inputFontFamily": "JetBrainsMono NFM Regular",
  "chat.editor.fontFamily": "JetBrainsMono NFM Regular",
  "debug.console.fontFamily": "JetBrainsMono NFM Regular",
  "editor.codeLensFontFamily": "JetBrainsMono NFM Regular",
  "notebook.output.fontFamily": "JetBrainsMono NFM Regular",
  "markdown.preview.fontFamily": "JetBrainsMono NFM Regular",
  "editor.inlayHints.fontFamily": "JetBrainsMono NFM Regular",
  // Editor
  "editor.fontSize": 12,
  "terminal.integrated.fontSize": 12,
  "editor.cursorSurroundingLines": 8,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.formatOnSave": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveWhenNoErrors": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "editor.largeFileOptimizations": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "git.replaceTagsWhenPull": true,
  "notebook.lineNumbers": "on",
  "workbench.layoutControl.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  // Windsurf
  "windsurf.autocompleteSpeed": "fast",
  // UI
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "auto",
  "editor.cursorSmoothCaretAnimation": "on",
  "window.titleBarStyle": "custom",
  "editor.scrollbar.horizontal": "auto",
  "editor.lineNumbers": "relative",
  "update.showReleaseNotes": false,
  "window.title": "${dirty}${activeEditorLong}${separator}${rootName}",
  "workbench.activityBar.location": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "explorer.compactFolders": true,
  "window.density.editorTabHeight": "compact",
  "workbench.editor.tabSizing": "shrink",
  "breadcrumbs.filePath": "off",
  "window.menuBarVisibility": "compact",
  "glassit.alpha": 245,
  // Vim
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#a9dc7660",
  "vim.highlightedyank.duration": 250,
  "vim.useCtrlKeys": true,
  "vim.statusBarColorControl": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-h>": false,
    "<C-f>": false,
    "<S-l>": false,
    "<S-j>": false,
    "<S-h>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    },
    {
      "before": [
        ","
      ],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    },
    {
      // delete character under cursor without copying it
      "before": [
        "x"
      ],
      "after": [
        "\"",
        "_",
        "x"
      ]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    },
    {
      "before": [
        ","
      ],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      // keep last yank when pasting
      "before": [
        "p"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "P"
      ]
    }
  ],
